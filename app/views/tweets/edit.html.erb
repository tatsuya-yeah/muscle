<h1>投稿編集</h1>
<%= form_for @tweet do |f| %>
<div class="field">
    <%= f.label :タイトル %>
    <%= f.text_field :title, :size => 30 %>
</div>

<div class="field">
    <%= f.label :部位 %>
    <%= f.text_field :part, :size => 30 %>
</div>
<div class="field">
    <%= f.label :種目 %>
    <%= f.text_field :training, :size => 30 %>
</div>
<div class="field">
    <%= f.label :内容 %>
    <%= f.text_field :about, :size => 30 %>
</div>
<%= f.submit "編集する" %>
<% end %>